{
  "arrowParens": "always",
  "bracketSpacing": true,
  "insertPragma": false,
  "bracketSameLine": true,
  "printWidth": 80,
  "proseWrap": "preserve",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "importOrderSeparation": true,
  "importOrderBuiltinModulesToTop": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrder": ["^@/(.*)$", "^../", "^./"],
  "endOfLine": "auto"
}
